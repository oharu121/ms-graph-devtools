  "repository": {
    "type": "git",
    "url": "git+https://github.com/oharu121/ms-graph-devtools.git"
  },
  "bugs": {
    "url": "https://github.com/oharu121/ms-graph-devtools/issues"
  },
  "homepage": "https://github.com/oharu121/ms-graph-devtools#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "axios-fluent": "^2.0.2",
    "dayjs": "^1.11.19",
    "jsdom": "^27.1.0"
